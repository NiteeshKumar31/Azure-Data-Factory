{
	"name": "Capture_Dataflow_Error_logs",
	"properties": {
		"activities": [
			{
				"name": "Error_Test_DataFlow",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "PipelineStartTime",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "Error_Test_DataFlow",
						"type": "DataFlowReference"
