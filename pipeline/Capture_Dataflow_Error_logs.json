				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "dbo.errorlogs",
					"storedProcedureParameters": {
						"Error_msg": {
							"value": {
								"value": "@if(equals(activity('Error_Test_DataFlow').Status, 'Failed'), activity('Error_Test_DataFlow').Error.message,'NULL' )\n\n",
								"type": "Expression"
							},
							"type": "String"
						},
						"Pipeline_Name": {
							"value": {
								"value": "@{activity('Error_Test_DataFlow').PipelineName}",
								"type": "Expression"
							},
							"type": "String"
						},
						"Pipeline_Runid": {
							"value": {
								"value": "@{activity('Error_Test_DataFlow').PipelineRunId}",
								"type": "Expression"
							},
							"type": "String"
						},
						"Pipeline_Runtime_minutes": {
							"value": {
								"value": "@utcNow()",
								"type": "Expression"
							},
							"type": "String"
						},
						"Pipeline_Status": {
							"value": {
								"value": "@variables('PipelineStatus')",
								"type": "Expression"
							},
							"type": "String"
						},
						"Pipeline_Trigger_Time": {
							"value": {
								"value": "@pipeline().TriggerTime",
								"type": "Expression"
							},
							"type": "DateTime"
						},
						"Rows_Read": {
							"value": {
								"value": "@if(equals(activity('Error_Test_DataFlow').Status, 'Succeeded'),activity('Error_Test_DataFlow').output.runStatus.metrics.sink1.sources.source1.rowsRead, 0 )\n",
								"type": "Expression"
							},
							"type": "Int32"
						},
						"Rows_Written": {
							"value": {
								"value": "@if(equals(activity('Error_Test_DataFlow').status, 'Succeeded'), activity('Error_Test_DataFlow').output.runStatus.metrics.sink1.rowsWritten, 0)\n",
								"type": "Expression"
							},
							"type": "Int32"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "linkedService1",
					"type": "LinkedServiceReference"
				}
			},
			{
